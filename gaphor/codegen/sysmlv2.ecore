      <eAnnotations source="http://www.omg.org/spec/SysML"/>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="aliasIds" upperBound="-1"
        eType="ecore:EDataType uml_types.ecore#//String">
