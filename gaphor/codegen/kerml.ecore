      </eAnnotations>
      <eAnnotations source="subsets" references="#//Type/ownedFeature"/>
      <eAnnotations source="http://www.omg.org/spec/SysML"/>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SelectExpression" eSuperTypes="#//OperatorExpression">
